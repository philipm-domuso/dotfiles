{
    "[python]": {
        "editor.formatOnSave": true
    },
    "cSpell.userWords": [
        "reddit"
    ],
    "csharpsortusings.sort.usings.splitGroups": false,
    "debug.toolBarLocation": "docked",
    "editor.fontFamily": "'Cascadia Code', 'Fira Code', Consolas, Monaco, Menlo, Monaco, 'Courier New', monospace",
    "editor.fontLigatures": true,
    "editor.fontSize": 11,
    "editor.linkedEditing": true,
    "editor.minimap.enabled": false,
    "editor.mouseWheelZoom": true,
    "editor.rulers": [
        80,
        100
    ],
    "editor.suggestSelection": "first",
    "emmet.triggerExpansionOnTab": true,
    "git.autofetch": true,
    "git.confirmSync": false,
    // "vim.useSystemClipboard": true,
    "git.enableSmartCommit": true,
    "gitlens.currentLine.enabled": false,
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.keymap": "alternate",
    "gruvboxMaterial.darkContrast": "soft",
    "gruvboxMaterial.lightContrast": "soft",
    "macros": {
        "saveAndExitVimInsertMode": [
            "workbench.action.files.save",
            "extension.vim_escape"
        ]
    },
    "mssql.connections": [
        {
            "authenticationType": "Integrated",
            "connectionString": "Server=(localdb)\\localdev;Integrated Security=True;Initial Catalog=discogs",
            "database": "discogs",
            "password": "",
            "profileName": "LocalDev-Discogs",
            "server": "(localdb)\\localdev",
            "user": ""
        }
    ],
    "python.formatting.blackArgs": [
        "--line-length",
        "120"
    ],
    "python.formatting.provider": "black",
    "python.jediEnabled": false,
    "python.languageServer": "Pylance",
    "python.linting.prospectorEnabled": true,
    "python.linting.pylintEnabled": false,
    "python.testing.pytestEnabled": true,
    "sqltools.connections": [],
    "sqltools.highlightQuery": false,
    "terminal.integrated.rendererType": "dom",
    "vim.easymotion": true,
    "vim.gdefault": true,
    "vim.insertModeKeyBindings": [
        {
            "after": [ "<Esc>" ],
            "before": [ "j", "j" ]
        },
        {
            "after": [],
            "before": [ "<C-e>" ],
            "commands": [
                { "command": "cursorEnd" }
            ]
        },
        {
            "after": [],
            "before": [ "<C-a>" ],
            "commands": [
                { "command": "cursorHome" }
            ]
        }
    ],
    "vim.useSystemClipboard": true,
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "window.autoDetectColorScheme": true,
    "window.menuBarVisibility": "toggle",
    "window.restoreWindows": "none",
    "window.titleBarStyle": "custom",
    "window.zoomLevel": 0,
    "workbench.activityBar.visible": true,
    "workbench.colorTheme": "Quiet Light",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "workbench.preferredDarkColorTheme": "Solarized Dark",
    "workbench.preferredLightColorTheme": "Solarized Light",
    "workbench.sideBar.location": "right",
    "debug.onTaskErrors": "showErrors",
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "editor.bracketPairColorization.enabled": true
}
